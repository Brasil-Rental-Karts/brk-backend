PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=brk_competition
DB_SSL=false

# JWT Configuration
JWT_SECRET=your-secret-key-change-in-production
JWT_ACCESS_EXPIRY=15m
JWT_REFRESH_EXPIRY=7d 

# RabbitMQ configuration (CloudAMQP)
RABBITMQ_URL=amqp://guest:guest@host:5672/vhost
RABBITMQ_EXCHANGE=database_events
RABBITMQ_QUEUE=database_changes
RABBITMQ_ROUTING_KEY=database.changes